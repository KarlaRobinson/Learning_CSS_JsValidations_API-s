  <h1>Welcome!</h1>
  <p>Let's shorten your Url.</p>

  <form action="/urls" method="post" >
      <input type="text" placeholder="url" name="long_url">
      <input type="submit" value="Go!">
  </form>


<br>
<br>
<br>
<fieldset>
  <legend>Try any site below:</legend>
  <br>
  <% @urls.each_with_index do |object, i| %>
    <a id="<%=object.id%>" href= "/<%=object.short_url%>" > <%=i+1%>. <%=object.short_url%>   Visted (<%=object.click_count%>) times.</a>

    <br>
  <% end %>

    <br>
</fieldset>


